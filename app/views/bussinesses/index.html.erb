<p id="notice"><%= notice %></p>

<h1>Instagram Businesses</h1>

<table class="table table-striped">
  <thead>
   
  </thead>

  <tbody>
    <div class="container">
     <div class="row no-gutter">
    <% @bussinesses.each do |bussiness| %>
      <tr>

	       <div class="col-lg-3"> 
	       <img src=<%= bussiness.photos %> class="img-responsive" alt="Responsive image">
		   <h4>Name:  <%= bussiness.name %></h4>
		   <h4>Rating:  <%= bussiness.ratings %></h4>
		   <h4>Review:  <%= bussiness.reviews %></h4>
		   <h4>Instalink:  <%= bussiness.instalink %></h4>
		   <h4>description:  <%=bussiness.description%></h4>

	       </div>
			
		
       
      </tr>
    <% end %>

 </div>
  </tbody>
</table>

<br>

<%= link_to 'New Bussiness', new_bussiness_path %>
